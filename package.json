{
  "name": "wanderlite",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "start:backend": "cd backend && .venv\\Scripts\\python.exe run_server.py",
    "start:frontend": "cd frontend && npm start",
    "dev": "concurrently \"npm: start:backend\" \"npm: start:frontend\"",
    "install:all": "npm --prefix frontend install && (cd backend && .venv\\Scripts\\python.exe -m pip install -r requirements.txt)",
    "health-check": "python scripts/health_check.py"
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1"
  }
}
